        function changemenuicon() {
            if ($("#SideMenuContentPlaceHolder_imenuicon").hasClass("fe-chevron-right") == true) {
                document.getElementById("SideMenuContentPlaceHolder_imenuicon").classList.add("fe-chevron-left");
                document.getElementById("SideMenuContentPlaceHolder_imenuicon").classList.remove("fe-chevron-right");
            }
            else {
                document.getElementById("SideMenuContentPlaceHolder_imenuicon").classList.remove("fe-chevron-left");
                document.getElementById("SideMenuContentPlaceHolder_imenuicon").classList.add("fe-chevron-right");
            }
        }

        function Employees_Populated(sender, e) {
            var autoList = $find("AutoCompleteEx").get_completionList();
            for (i = 0; i < autoList.childNodes.length; i++) {
                var imgeUrl = autoList.childNodes[i]._value;
                var text = autoList.childNodes[i].firstChild.nodeValue;
                autoList.childNodes[i]._value = text;
                autoList.childNodes[i].innerHTML = imgeUrl;
            }
        }

        function OnEmployeeSelected(source, eventArgs) {
            var idx = source._selectIndex;
            sessionStorage.setItem("searchitem", eventArgs.get_text())
            SetSession();
            $get(idx).click();
        }

        function SetSession() {
            var name = sessionStorage.getItem("searchitem");
            var request;
            if (window.XMLHttpRequest) {
                //New browsers.
                request = new XMLHttpRequest();
            }
            else if (window.ActiveXObject) {
                //Old IE Browsers.
                request = new ActiveXObject("Microsoft.XMLHTTP");
            }
            if (request != null) {
                var url = "../../UI/Default.aspx/SetSession";


                request.open("POST", url, false);
                var params = "{name: '" + name + "'}";
                request.setRequestHeader("Content-Type", "application/json");
                request.onreadystatechange = function () {
                    if (request.readyState == 4 && request.status == 200) {
                        // alert(JSON.parse(request.responseText).d);
                    }
                };
                request.send(params);
            }
        }

       
    </script>

