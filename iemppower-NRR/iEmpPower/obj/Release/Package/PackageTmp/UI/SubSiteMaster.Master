        function menuload() {
            ! function (t) {
                "use strict";
                window.jQuery.App.init()
            }(), Waves.init();
        }

        function redct(type) {
            if (type == "PR") {
                window.location.href = "/UI/PR/Purchase_Requisitions.aspx";
            }
            else if (type == "IEXP") {
                window.location.href = "/UI/Benefits_Payment/iExpense_Request.aspx";
            }
            else if (type == "TR") {
                window.location.href = "/UI/Benefits_Payment/Travel_Requests.aspx";
            }
        }
    </script>


