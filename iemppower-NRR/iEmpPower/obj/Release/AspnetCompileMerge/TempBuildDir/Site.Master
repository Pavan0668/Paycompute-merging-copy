            </aside>
            <!-- #END# Right Sidebar -->
        </section>
        <div>
            <section class="content">
                <div class="container-fluid">
                    <div class="row clearfix">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <div class="card">
                                <asp:ContentPlaceHolder ID="MainContent" runat="server">
                                </asp:ContentPlaceHolder>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
       
        <div id="divChrt" style="padding: 9px; padding-left: 70px; display: none">
            <div style="border: inset; border-color: black; font-size: small; padding: 2px;">
                <asp:LinkButton ID="lnkOrgChrt" ForeColor="#00617c" runat="server" OnClick="linklblRepPERNR_Click">View Org Chart</asp:LinkButton>
            </div>
        </div>

        <asp:LinkButton ID="lnkDummyAnnouncement" runat="server"></asp:LinkButton>
        <Ajx:ModalPopupExtender ID="mpAnnouncement" runat="server" PopupControlID="PnlAnnouncememt" TargetControlID="lnkDummyAnnouncement" BehaviorID="mpeAnnouncement"
            BackgroundCssClass="modalBackground">
        </Ajx:ModalPopupExtender>
        <div id="divpnl" runat="server" style="display: none">
            <asp:Panel runat="server" ID="PnlAnnouncememt" Width="70%" Height="400px" ScrollBars="Vertical">

                <h2 style="padding-left: 6px">Announcements</h2>


                <div class="DivSpacer01" style="padding-left: 10px">
                    <asp:GridView ID="GV_Announcement" runat="server" AutoGenerateColumns="False" Width="98%" CssClass="Grid leftpad" GridLines="Both" PagerStyle-CssClass="cssPager"
                        CellPadding="100" CellSpacing="5" ForeColor="#333333" Font-Size="8" Font-Names="verdana" BorderWidth="1px"
                        BorderColor="#03345b" BorderStyle="Solid">
                        <Columns>
                            <asp:TemplateField HeaderText="Slno" Visible="false">
                                <ItemTemplate>
                                    <asp:Label ID="LblSlno" runat="server" Text="<%# Container.DataItemIndex+1 %>"></asp:Label>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <asp:BoundField DataField="ATITLE" HeaderText="Title" SortExpression="ATITLE" />
                            <asp:BoundField DataField="ADESC" HeaderText="Desc" SortExpression="ADESC" />
                            <asp:BoundField DataField="ADATE" HeaderText="From Date" SortExpression="ADATE" DataFormatString="{0:dd-MMM-yyyy}" />
                            <asp:BoundField DataField="TOADATE" HeaderText="To Date" SortExpression="TOADATE" DataFormatString="{0:dd-MMM-yyyy}" />
                            <asp:BoundField DataField="ATIME" HeaderText="time" />
                            <asp:BoundField DataField="APLACE" HeaderText="place" SortExpression="APLACE" />
                            <%--<asp:BoundField DataField="CREATED_BY" HeaderText="Created by" SortExpression="CREATED_BY" />--%>
                            <asp:TemplateField HeaderText="Created by" SortExpression="CREATED_BY">
                                <ItemTemplate>
                                    <%#Eval("CREATED_BY") + "-"+Eval("ENAME")%>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <asp:BoundField DataField="CREATED_ON" DataFormatString="{0:dd-MMM-yyyy HH:mm:ss}" HeaderText="Created On" SortExpression="CREATED_ON" />
                        </Columns>
                        <RowStyle BackColor="#f4f8f9" Font-Size="8" Font-Names="verdana" Height="30px" />
                        <HeaderStyle BackColor="#00617C" Font-Bold="True" ForeColor="White" Font-Size="8" Font-Names="verdana" Height="30px" />
                        <AlternatingRowStyle BackColor="White" Height="30px" />
                        <PagerStyle BackColor="#00617C" ForeColor="White" HorizontalAlign="Center" Font-Size="8" Font-Names="verdana" />
                        <SelectedRowStyle BackColor="#afd4e0" Font-Bold="True" ForeColor="#333333" Height="30px" />
                        <FooterStyle BackColor="White" Font-Bold="True" ForeColor="White" Font-Size="9" Font-Names="verdana" />
                    </asp:GridView>
                </div>
                <div class="DivSpacer01" style="padding-left: 10px">
                    <asp:Button ID="btnClose" runat="server" Text="Close" OnClientClick="return HideModalPopupAnnouncement()" />
                </div>
            </asp:Panel>
        </div>
        <%--   </div>
        </asp:Panel>--%>
    </form>

</body>
</html>
