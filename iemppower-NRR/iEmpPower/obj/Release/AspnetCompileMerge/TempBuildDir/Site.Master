    <script type="text/javascript">
        function preventBack() { window.history.forward(); }
        setTimeout("preventBack()", 0);
        window.onunload = function () { null };
        function DisplayNameAnnouncement() {
            $find("mpeAnnouncement").show();
            return false;
        }
        function HideModalPopupAnnouncement() {
            $find("mpeAnnouncement").hide();
            return false;
        }

    </script>


    <style type="text/css">
        .gridview td
        {
            padding: 5px 10px 2px 0px !important;
        }

        .gridview th
        {
            padding: 5px 10px 2px 0px !important;
        }

        #PnlAnnouncememt
        {
            display: none;
            border-color: black;
            border-style: solid;
            background-color: white !important;
            /*right:25px !important;*/
        }       
        .modalBackground
        {
            background-color: black;
            filter: alpha(opacity=90);
            opacity: 0.8;
        }

        .modalPopup
        {
            background-color: #FFFFFF;
            border-width: 3px;
            border-style: solid;
            border-color: black;
            height: 500px;
        }

        .lnkbtn
        {
            display: none;
            visibility: hidden;
        }

        .modal-content
        {
           
            z-index: -1;
            background-color: rgba(0,94,124,0.9);
            padding: 5px;
            border: 2px solid #888;
            width: 15%; /*Could be more or less, depending on screen size*/
        }

        .hidden
        {
            visibility: hidden;
        }
    </style>

    <script type="text/javascript">
        function signout() {
            localStorage.clear();
            sessionStorage.clear();
            document.getElementById("lnkBtnSignout1").click();
